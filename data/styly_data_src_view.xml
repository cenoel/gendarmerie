<odoo>
  <data>
    <template id="my_assets_backend" name="web_gn assets backend" inherit_id="web.assets_backend">
      <xpath expr="//link[@href='/web/static/src/less/bootstrap_overridden.less']" position="replace">
        <link rel="stylesheet" type="text/less" href="/og_gn/static/src/less/bootstrap_overridden.less"/>
      </xpath>
      <xpath expr="//link[@href='/web/static/src/less/fields_extra.less']" position="replace">
        <link rel="stylesheet" type="text/less" href="/og_gn/static/src/less/fields_extra.less"/>
      </xpath>
    </template>

    <template id="my_less_helpers" name="web_gn less helpers" inherit_id="web.less_helpers">
      <xpath expr="//link[@href='/web/static/src/less/variables.less']" position="replace">
        <link rel="stylesheet" type="text/less" href="/og_gn/static/src/less/variables.less"/>
      </xpath>
    </template>
  </data>
</odoo>